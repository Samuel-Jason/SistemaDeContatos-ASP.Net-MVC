@{
    ViewData["Title"] = "Editar Contato";
}

<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>

<form>
    <div class="mb-3">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholde="Digite o nome do contato">
    </div>
    <div class="mb-3">
        <label for="email">Email</label>
        <input type="text" class="form-control" placeholde="Digite o email do contato">
    </div>
    <div class="mb-3">
        <label for="celular">Celular</label>
        <input type="text" class="form-control" placeholde="Digite o celular do contato">
    </div>

    <button type="submit" class="btn btn-primary btn-block">Salvar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>

</form>